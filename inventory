[jenkins]
ansible-jenkins-ssh

[docker]
ansible-docker-ssh
ansible-jenkins-ssh

[swarm-master]
ansible-docker-ssh

[jenkins:vars]
ansible_user=qa-admin
ansible_ssh_private_key_file=~/.ssh/ansible_id_rsa
ansible_ssh_common_args='-o StrictHostKeyChecking=no'

[docker:vars]
ansible_user=qa-admin
ansible_ssh_private_key_file=~/.ssh/ansible_id_rsa
ansible_ssh_common_args='-o StrictHostKeyChecking=no'

[swarm-master:vars]
ansible_user=qa-admin
ansible_ssh_private_key_file=~/.ssh/ansible_id_rsa
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
